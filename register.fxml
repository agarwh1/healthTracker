      </Label>
      <Label layoutX="256.0" layoutY="72.0" opacity="0.4" prefHeight="14.0" prefWidth="120.0" text="Takes less than a minute">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </Label>
      <ImageView fx:id="bannerImageView2" fitHeight="415.0" fitWidth="237.0" layoutX="-4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../Logo/Banner2.png" />
         </image>
      </ImageView>
      <Label layoutX="255.0" layoutY="103.0" prefHeight="17.0" prefWidth="120.0" text="Firstname">
         <font>
            <Font name="Georgia" size="9.0" />
         </font>
      </Label>
      <TextField fx:id="firstnameTextField" layoutX="257.0" layoutY="120.0">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </TextField>
      <Label layoutX="255.0" layoutY="253.0" prefHeight="17.0" prefWidth="120.0" text="Password">
         <font>
            <Font name="Georgia" size="9.0" />
         </font>
      </Label>
      <PasswordField fx:id="setPasswordField" layoutX="253.0" layoutY="270.0">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </PasswordField>
      <Button fx:id="registerButton" layoutX="254.0" layoutY="360.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefHeight="21.0" prefWidth="66.0" style="-fx-background-color: #15161b;" text="Register" textFill="WHITE">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </Button>
      <Label layoutX="255.0" layoutY="153.0" prefHeight="17.0" prefWidth="120.0" text="Lastname">
         <font>
            <Font name="Georgia" size="9.0" />
