      </Label>
      <TextField fx:id="lastNameTextField" layoutX="255.0" layoutY="170.0">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </TextField>
      <Label layoutX="255.0" layoutY="203.0" prefHeight="17.0" prefWidth="120.0" text="Username">
         <font>
            <Font name="Georgia" size="9.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="254.0" layoutY="220.0">
         <font>
            <Font name="Georgia" size="11.0" />
         </font>
      </TextField>
      <Label layoutX="255.0" layoutY="303.0" prefHeight="17.0" prefWidth="120.0" text="Repeat Password">
         <font>
            <Font name="Georgia" size="9.0" />
