            <Font name="Georgia" size="9.0" />
         </font>
         <graphic>
            <ImageView fx:id="backImageView" fitHeight="11.0" fitWidth="11.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../../Icons/54782.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="registerMessageLabel" layoutX="395.0" layoutY="16.0" textFill="#0e8366">
         <font>
            <Font name="Georgia" size="9.0" />
