<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.iacode.GraphController">
   <children>
      <LineChart fx:id="lineChart" layoutX="43.0" layoutY="8.0" title="BMI over time">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="backButton" graphicTextGap="2.0" layoutX="12.0" layoutY="17.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="20.0" prefWidth="46.0" style="-fx-background-color: #FFFFFF;" text="Back">
         <font>
            <Font name="Georgia" size="9.0" />
         </font>
      </Button>
      <ImageView fx:id="backImageView" fitHeight="11.0" fitWidth="11.0" layoutX="12.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../Icons/54782.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
